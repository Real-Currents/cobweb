          <field accessType='inputOnly' type='SFVec3f' name='set_hitPoint'/>
          <field accessType='inputOnly' type='SFTime' name='set_time'/>
          <field accessType='initializeOnly' type='MFNode' name='textures'>
            <ImageTexture USE='_2'/>
            <ImageTexture USE='_1'/>
            <ImageTexture USE='_3'/>
          </field>
<![CDATA[ecmascript:

var numCircles = 75;
var zAxis      = new SFVec3f (0, 0, 1);
var materials  = [ ];


function initialize ()
{
	var
		currentScene = Browser .currentScene,
		group        = currentScene .getNamedNode ("Circles");

	group .children .length = 0;

	for (var i = 0; i < numCircles; ++ i)
	{
		var
			transform  = currentScene .createNode ("Transform"),
			shape      = currentScene .createNode ("Shape"),
			appearance = currentScene .createNode ("Appearance"),
			material   = currentScene .createNode ("Material"),
			disk       = currentScene .createNode ("Disk2D");
		
		group .children [i]     = transform;
		transform .children [0] = shape;
		shape .appearance       = appearance;
		shape .geometry         = disk;
		appearance .material    = material;
		material .diffuseColor  = new SFColor ();
		material .emissiveColor = new SFColor (1, 1, 1);
		material .transparency  = i / (numCircles - 1);

		materials .push (material);

		place (transform);
	}
}

function set_hitPoint (value)
{
	value .z = 0;
	
	var group = Browser .currentScene .getNamedNode ("Circles");

	for (var i = 0; i < numCircles; ++ i)
	{
		var
			transform = group .children [i],
			direction = transform .translation .subtract (value),
			distance  = direction .length ();
			
		direction = direction .normalize () .multiply (1 / (Math .pow (distance * 50, 2)));
		
		transform .translation = transform .translation .add (direction);
	}
}

function set_time ()
{
	var group = Browser .currentScene .getNamedNode ("Circles");
	
	for (var i = 0; i < numCircles; ++ i)
	{
		var
			dt        = 1 / Browser .currentFrameRate,
			transform = group .children [i],
			material  = transform .children [0] .appearance .material;
		
		material .transparency += dt / numCircles * 4;
		transform .rotation     = new SFRotation (zAxis, material .transparency * Math .PI * 2 * 8);
		
		if (material .transparency > 1)
		{
			material .transparency = 0;
			place (group .children [Math .round (random (0, numCircles - 1))]);
		}
	}
}
	
function place (transform)
{
	var appearance = transform .children [0] .appearance;

	transform .translation .x = random (-2, 2);
	transform .translation .y = random (-1, 1);
	transform .rotation       = new SFRotation (zAxis, Math .random () * 2 * Math .PI);
	transform .scale .x       = random (0.05, 0.2);
	transform .scale .y       = transform .scale .x;

	appearance .texture = textures [Math .round (Math .random () * 2)];
}

function random (min, max)
{
	return Math .random () * (max - min) + min;
}]]> 
